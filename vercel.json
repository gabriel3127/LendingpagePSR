{
  "redirects": [
    {
      "source": "http://santaritaembalagens.com.br/(.*)",
      "destination": "https://santaritaembalagens.com.br/$1",
      "permanent": true
    },
    {
      "source": "http://www.santaritaembalagens.com.br/(.*)",
      "destination": "https://www.santaritaembalagens.com.br/$1", 
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        }
      ]
    }
  ]
}